{
  "name": "flutter-assets",
  "displayName": "Flutter Assets",
  "description": "Automatically generate a dart class for managing Flutter assets.",
  "version": "0.0.1",
  "publisher": "goweii",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#31b9f5",
    "theme": "dark"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/goweii/flutter-assets/issues",
    "email": "goweii@163.com"
  },
  "homepage": "https://github.com/goweii/flutter-assets/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/goweii/flutter-assets.git"
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flutter-assets.generate-now",
        "title": "Flutter Assets: Generate Now"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "ts-loader": "^9.4.2",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1",
    "@vscode/test-electron": "^2.3.0"
  },
  "dependencies": {
    "yaml": "^1.10.0"
  }
}